<Window x:Class="Recert.UI.PrinterDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Recert.UI"
        mc:Ignorable="d"
        Title="Select Printer" Height="401.413" Width="282.905">
	<Grid>
		<ListBox x:Name="listBox" HorizontalAlignment="Left" Height="294" Margin="10,41,0,0" VerticalAlignment="Top" Width="255"/>
		<Button x:Name="printButon" Content="Print" HorizontalAlignment="Left" Margin="10,340,0,0" VerticalAlignment="Top" Width="126" Click="PrintButon_Click"/>
		<Label x:Name="label" Content="Please, select your printer:" HorizontalAlignment="Left" Margin="5,10,0,0" VerticalAlignment="Top" Width="255"/>
		<Button x:Name="cancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="139,340,0,0" VerticalAlignment="Top" Width="126" Click="CancelButton_Click"/>

	</Grid>
</Window>
